{
    "extends": ["wesbos"],
    "plugins": ["@typescript-eslint"],
    "rules": {
        "react/jsx-filename-extension": [2, { "extensions": [".tsx"] }],
        "react/jsx-props-no-spreading": 0,
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 80,
                "tabWidth": 4,
                "semi": true
            }
        ],
        "import/no-unresolved": "off"
    }
}
